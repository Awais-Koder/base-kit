{
    "name": "awais/base-kit",
    "description": "This is base kit for other packages.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Awais\\BaseKit\\": "src"
        }
    },
    "require": {
        "php": "^8.1",
        "doctrine/dbal": "^3.7",
        "laravel/pulse": "^1.0@beta",
        "guzzlehttp/guzzle": "^7.2",
        "infyomlabs/adminlte-templates": "^6.0",
        "infyomlabs/laravel-generator": "^6.0",
        "infyomlabs/laravel-ui-adminlte": "^5.2",
        "laravel/framework": "^10.10",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.8",
        "symfony/http-client": "^6.3",
        "symfony/postmark-mailer": "^6.3",
        "barryvdh/laravel-debugbar": "^3.9",
        "fakerphp/faker": "^1.9.1",
        "kitloong/laravel-migrations-generator": "^6.11",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "laravel/telescope": "^4.17",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.9",
        "fakerphp/faker": "^1.9.1",
        "kitloong/laravel-migrations-generator": "^6.11",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "laravel/telescope": "^4.17",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    
    "extra": {
        "laravel": {
            "providers": [
                "Awais\\BaseKit\\BaseKitServiceProvider"
            ],
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    }
}